<h1>Edit Job Group</h1>
<%= form_for @job_group, :url => { :action => "edit_job_group", :id => @group.id, :j_id => @job_group_new.id } do |f| %>
    <%= render :partial => 'job_group_form', :locals => { :f => f } %>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
<%= link_to 'Back', admin_groups_path(:id => @group.id) %>
